        <!-------------------------------------------Common Validation VF Page -------------------------------->
        <apex:include pageName="NMIMS_Registration_Form_MBA_Validation"/>
  
        <script>
            $(document).ready(function(){

                var expiredDate = '{!JSENCODE($Label.Conneqt_Expired_date)}';
                console.log('expiredDate==>'+expiredDate);
                //var date = moment(expiredDate).add(1, 'days').calendar();
                var expdate = moment(expiredDate,'YYYY-MM-DD');
                var now = moment();
                console.log('expiredDate '+expdate.format('DD-MM-YYYY HH:ss')+' todaydate '+now.format('DD-MM-YYYY hh:ss'));
                console.log('now.diff(date) '+expdate.diff(now));
                
                if (expdate.diff(now, 'days') >= 0) {
                    console.log('not expired..');
                    $('.regWindowdiv').css("display","none");
                    $('.saveProceedBtn').css("display","block");
                } else {
                    console.log('expired...');
                    $('.saveProceedBtn').css("display","none");
                    $('.regWindowdiv').css("display","block");
                }

            // show Error Message Popup to User
                if('{!strSecToShow}' =='PopUpError'){
                    $('#smallModal').modal('show');
                }

            /*------------------------------------------------After ReLogin check upto which tab details fill based upon that enabled tab -------*/     
                if('{!IsReLogin}' =='true'){
                    $('[id$=generateRegistrationNoTab]').addClass('disabled');
                    var flag = validateForm('[id$=personalInformation]');
                    onClickNextToPersonalInformationTab();
                    if(flag !=0){
                        $('[id$=personalInformationTab]').removeClass('disabled');
                    }else{
                        flag = validateForm('[id$=addressInformation]');
                        if (flag != 0) {
                            $('[id$=personalInformationTab]').removeClass('disabled');
                            $('[id$=addressInformationTab]').removeClass('disabled');
                        }else{
                            flag = validateForm('[id$=programDetails]');
                            if (flag != 0) {
                                $('[id$=personalInformationTab]').removeClass('disabled');
                                $('[id$=addressInformationTab]').removeClass('disabled');
                                $('[id$=programDetailsTab]').removeClass('disabled');
                            }else{
                                flag = validateForm('[id$=academicInformation]');
                                if (flag != 0) {
                                    $('[id$=personalInformationTab]').removeClass('disabled');
                                    $('[id$=addressInformationTab]').removeClass('disabled');
                                    $('[id$=programDetailsTab]').removeClass('disabled');
                                    $('[id$=academicInformationTab]').removeClass('disabled');
                                }else{
                                    flag = validateForm('[id$=workExperience]');
                                    if (flag != 0) {
                                        $('[id$=personalInformationTab]').removeClass('disabled');
                                        $('[id$=addressInformationTab]').removeClass('disabled');
                                        $('[id$=programDetailsTab]').removeClass('disabled');
                                        $('[id$=academicInformationTab]').removeClass('disabled');
                                        $('[id$=workExperienceTab]').removeClass('disabled');
                                    }else{
                                        $('[id$=personalInformationTab]').removeClass('disabled');
                                        $('[id$=addressInformationTab]').removeClass('disabled');
                                        $('[id$=programDetailsTab]').removeClass('disabled');
                                        $('[id$=academicInformationTab]').removeClass('disabled');
                                        $('[id$=workExperienceTab]').removeClass('disabled');
                                    }
                                }
                            }
                        }
                    }
                }
                
                if('{!sectionName}' == 'Generate Registration No' ){
                    onClickNextToPersonalInformationTab();
                }else if('{!sectionName}' == 'Personal Information' ){
                    onClickNextToAddressInformationTab();
                }
                
                <!--------------------------------click from Confirmation Page----------------->
                if('{!sectionName}' == 'Personal Information' && '{!IsEditFormConfirmation}' == 'true'){
                    onClickNextToPersonalInformationTab();
                }else if('{!sectionName}' == 'Address Information' && '{!IsEditFormConfirmation}' == 'true'){
                    onClickNextToAddressInformationTab();
                }else if('{!sectionName}' == 'Program Details' && '{!IsEditFormConfirmation}' == 'true'){
                    onClickNextToProgramDetailsTab();
                }else if('{!sectionName}' == 'Academic Information' && '{!IsEditFormConfirmation}' == 'true'){
                    onClickNextToAcademicInformationTab();
                }else if('{!sectionName}' == 'Work Experience' && '{!IsEditFormConfirmation}' == 'true'){
                    onClickNextToWorkExperienceTab();
                }
                
                $('.industry').change(function () {
                    var _optionVal = $(this).val();
                    var _indexVal = $(".industry").index(this);
                    if(_indexVal == 0 && _optionVal != "ITES/ BPO / CRM / Transcription") {
                        showHtmlMessage('Invalid Details','First Work Experience of Industry should be "ITES/ BPO / CRM / Transcription".');
                        $(this).val('');
                    }
                });
                
            });
            
        </script>
