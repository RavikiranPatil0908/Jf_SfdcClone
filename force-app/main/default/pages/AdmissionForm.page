                                    </apex:repeat>
                                    <br/>
                                    <apex:inputText id="otp" rendered="{!IF(objOtp.optStatus == 'OTP Sent',true,false)}" styleClass="form-control numonly" value="{!objOtp.Otp}"
                                     html-placeholder="Enter OTP.." />
                                </div>
                                <div class="modal-footer" style="{!IF(objOtp.optStatus == 'OTP Sent', 'display:block', 'display:none')}">
                                    <apex:commandButton value="Resend OTP" styleClass="btn btn-primary sendBtn  pull-left disabled"  id="sendBtn" onclick="sendOTP();" rerender="otpPopUp"
                                        />
                                    <a  class="btn btn-danger  pull-left" id="otpLogout" href="/nmlogin_new?&type=registration"
                                        >Log Out</a>
                                    <apex:commandButton value="Confirm" styleClass="btn btn-primary pull-right" onclick="if(validate()) return false;" id="verifyBtn"
                                    rendered="{!IF(objOtp.optStatus == 'OTP Sent',true,false)}" rerender="otpPopUp"
                                    />
                                    <apex:actionFunction name="verifyOtp" action="{!VerifyOTP}"  />
                                    <apex:actionFunction name="validateOTPStatus" action="{!validateOTPStatus}"   />
                                    <div class="container-fluid" style="margin: 10px 0;" id="divTimer">
                                        <div class="row">
                                            <span style="float: left;">If you haven't recieved the OTP in
                                                <span class="showTimer"></span>, click resend</span><span class="timer hidden"></span>
                                        </div>
                                    </div>
                                    <apex:outputPanel rendered="{!objOtp.OtpMode == 'E'}" layout="block" styleClass="container-fluid" style="margin: 10px 0;">
                                        <div class="row">
                                            <span style="float: left;">Note: Kindly check Spam Folder for email OTP</span>
                                        </div>
                                    </apex:outputPanel>
                                </div>
                                <div class="modal-footer" style="{!IF(objOtp.optStatus == '903' || objOtp.optStatus == '907', 'display:block', 'display:none')}">
                                    <apex:commandButton value="Resend OTP" styleClass="btn btn-primary pull-left" id="sendBtn2" onclick="sendOTP();"
                                     reRender="otpPopUp" />
                                </div>
                                <apex:inputHidden value="{!objOtp.resendOtp}" id="hdnResendCode" />
                            </apex:form>
                        </div>
                    </div>
                </div>
                <script>

                    $(document).ready(function (){
                        //For the OTP Timer.
                    })
                    function fmtMSS(s){return(s-(s%=60))/60+(9<s?':':':0')+s}
                    function otpTimer() {
                        $('.timer').html(timeVal);
                        $('.showTimer').html(fmtMSS(timeVal));
                        clearInterval(loadTimer);
                        loadTimer = setInterval(refreshTimer, timerInterval);
                        refreshTimer();
                    }

                    // Alloted time in seconds
                    // var jsTime = 180 - Math.trunc(Math.abs((parseInt(new Date().getTime()) /1000) -  ((parseInt('{!objOTP.otpSentTime}')) / 1000)));
                    // var jsTime = (parseInt('{!objOTP.otpSentTime}')/ 1000 + 180) - parseInt(new Date().getTime()) /1000;
                    // console.log(jsTime)
                    // var timeVal = !isNaN(jsTime) && jsTime > 0 &&  jsTime<= 180 ? jsTime : 0;

                    let timeDiff = Math.trunc((parseInt(new Date().getTime()) - parseInt('{!objOTP.otpSentTime}')) / 1000);

                    console.log('Time --> ',timeDiff <=0 ? 180 : (180 - timeDiff) > 0 ? 180 - timeDiff : 0 , timeDiff);
                    var timeVal = timeDiff <=0 ? 180 : (180 - timeDiff) > 0 ? 180 - timeDiff : 1 ;
                    $('.timer').html(timeVal);
                    $('.showTimer').html(fmtMSS(timeVal));
                    refreshTimer();

                    // Interval
                    var timerInterval = 1000;
                    var loadTimer = setInterval(refreshTimer, timerInterval);

                    // Refresh Timer
                    function refreshTimer() {
                        setTimeout(function () {
                            var myTime = parseInt($('.timer').html()) - 1;
                            if ( myTime > 0) {
                                $('.timer').html(myTime);
                                $('.showTimer').html(fmtMSS(myTime));
                            } else {
                                $('.timer').html('');
                                $('.showTimer').html('');
                                $('#divTimer').addClass('hidden');
                                // $(".sendBtn").disabled = false
                                $(".sendBtn").removeClass('disabled');
                                $(".sendBtn").prop("disabled", false);
                                // $("[id$=sendBtn]").removeClass('disabled');
                            }
                        }, 1);
                    }
                    $('[id$=otp]').val('');
                    otpTimer();
                    function sendOTP() {
                        console.log('sendOTP called--> ' );
                        $('[id$=hdnResendCode]').val('true');
                        validateOTPStatus();
                    }
                    function onOtpLogout() {
                        window.location.href = "/nmlogin_new?&type=registration";
                    }
                    function validate() {
                        if ($('[id$=otp]').val()) {
                            // $('.otpPopUp').hide();
                            verifyOtp();
                            $('.loaderPanel').show();
                            return false;
                        }
                        return true;
                    }
                </script>
            </apex:outputPanel>
        </apex:outputPanel>

