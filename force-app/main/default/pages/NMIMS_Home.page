 /* MultiStep Progress Bar */
 .container-fluid {
 
  overflow-x: auto;
}

.multi-steps {
  display: flex;
  justify-content: flex-start;
  align-items: flex-start;
  padding: 0;

  margin-top: 10px;
  margin-left: 30px
  list-style: none;
  position: relative;
  counter-reset: step;
}

.multi-steps::before {
  content: '';
  position: absolute;
  top: 19px; /* Adjust to be midway between circles */
  left: 0;
  width: 87%;
  margin-left: 55px;
  height: 2px;
  background: #e0e0e0;
  z-index: 0;
}

.multi-steps li {
  position: relative;
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-right: 11%;
  z-index: 1;
}

.multi-steps li .circle {
  display: flex;
  align-items: center;
  justify-content: center;
  height: 38px;
  width: 38px;
  background: white;
  border: 2px solid #e0e0e0;
  border-radius: 50%;
  font-size: 14px;
  color: #999;
  transition: all 0.2s ease;
}

.multi-steps li.is-active .circle {
  background: #057724;
  color: white;
  border-color: #057724;
}

.multi-steps li .label {
  margin-top: 10px;
  font-size: 12px;
  color: #999;
  text-align: center;
}

.multi-steps li.is-active .label {
  color:  #057724;
}

/* Ensure it is scrollable */
.container-fluid {
  overflow-x: auto;
  white-space: nowrap;
}

/* Responsive styling */
@media (max-width: 667px) {
  .multi-steps {
   
    flex-direction: row;
    justify-content: flex-start;
  }

  .multi-steps::before{
    width: 235%;
  }
}



/* track progress bar css */
.progress-container {
    display: flex;
    justify-content: center;
    padding-left: 5%;
    padding-right: 5%;
    margin-top: 20px;
    max-width: 100%;
    width: 100%;
    overflow-x: auto; /* Enables horizontal scrolling */
    overflow-y: hidden;
    white-space: nowrap;
}

.progress-container .steps {
    display: flex;
    width: 100%;

    align-items: center;
    justify-content: space-between;
    position: relative;
}

.steps .step {
    display: flex;
    flex-direction: column;
    align-items: center;
}

.steps .circle {
    display: flex;
    align-items: center;
    justify-content: center;
    height: 35px;
    width: 35px;
    color: #999;
    font-size: 22px;
    font-weight: 500;
    border-radius: 50%;
    background: #fff;
    border: 2px solid #e0e0e0;
    transition: all 200ms ease;
    transition-delay: 0s;
    z-index: 1;
}

.steps .circle.active {
    transition-delay: 100ms;
    background-color: green;
    border-color: #5676cd;
    color: #eaf0ed;
}

.steps .label {
    margin-top: 8px;
    font-size: 14px;
    color: #999;
}

.steps .progress-bar {
    position: absolute;
    top: 26%;
    transform: translateY(-50%);
    height: 1px;
    width: 100%;
    background: #b6b7b8;
    z-index: 0;
}

.progress-bar .indicator {
    position: absolute;
    height: 100%;
    width: 0%;
    background: #4070f4;
    transition: all 300ms ease;
}

/* Responsive styling */
@media (max-width: 768px) {
    .progress-container {
        max-width: 200%;
        width: 150%;
        overflow-x: auto; /* Enables horizontal scrolling */
        overflow-y: hidden;
        white-space: nowrap;
    }
}


/* Circular progress css */

*{
    margin: 0;
    padding: 0;
    box-sizing: border-box;
    font-family: 'Poppins', sans-serif;
}

.chart-container{
    display: flex;
   
    width: 100%;
    padding: 50px 0;
    border-radius: 8px;
    background: #fff;
    row-gap: 30px;
    flex-direction: column;
    align-items: center;
}
.circular-progress{
    position: relative;
    height: 200px;
    width: 200px;
    border-radius: 50%;
    background: conic-gradient(#7d2ae8 120deg, #ededed 0deg);
    display: flex;
    align-items: center;
    justify-content: center;
}

.circular-progress{
    position: relative;
    height: 200px;
    width: 200px;
    border-radius: 50%;
    background: conic-gradient(#7d2ae8 120deg, #ededed 0deg);
    display: flex;
    align-items: center;
    justify-content: center;
}
.circular-progress::before{ 
    content: "";
    position: absolute;
    height: 170px;
    width: 170px;
    border-radius: 50%;
    background-color: #fff;
}
.progress-value{
    position: relative;
    font-size: 40px;
    font-weight: 600;
    color: #7d2ae8;
}
.text{
    font-size: 30px;
    font-weight: 500;
    color: #606060;
}
