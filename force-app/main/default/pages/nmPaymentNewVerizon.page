            return (e.which != 8 && e.which != 0 && (e.which < 48 || e.which > 57) ) ? false : true;
        });
        
        var installmentAmountPaid = '{!payOptions.Option1.AlreadyPaidInstallmentAmt}';
        var installmentPayFor = '{!payOptions.Option1.InstallmentPayFor}';
        var isReRegistration = '{!IsReregistartion}';
        if(parseInt(installmentAmountPaid) > 0 && '{!noOfInstallmentAllowed}' !='' && '{!noOfInstallmentAllowed}' !=null)
        {
            if('{!mapOfConfigurePayOpt['ChooseProgram']}' !='Executive Programs' && isReRegistration =='false'){
                $('[id$=NoteToDisplayForInstallment]').show();
            }else if (isReRegistration =='false' && '{!mapOfConfigurePayOpt['ChooseProgram']}' =='Executive Programs'){
                $('[id$=NoteToDisplayForInstallmentSAS]').show();
            } 
           else {
               $('[id$=NoteToDisplayForInstallmentReReg]').show();
           }   
            
            $('[id$=InsallmentPayOpt]').show();
            $('[id$=SecondInstallment]').show();
            $('[id$=installmentId]').prop('checked', true );
            $('[id$=hdnInstallmentPaymentOption]').val(installmentPayFor)
            $('[id$=hdnPaymentOption]').val('Installment');
            if(installmentPayFor =='admission' && isReRegistration =='false'){
                var minAmountToPay = '{!payOptions.Option2.minAmountToPay}';
