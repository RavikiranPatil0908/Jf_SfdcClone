                $('[id$=InstallmentAmountValidateForAdmission]').val(minAmountToPay);
                document.getElementById("amountToBePaid").innerHTML = '{!payOptions.Option2.Amount}'-minAmountToPay;                    
                $('[id$=balanceAmount]').show();
                $('[id$=hdnInstallmentAmount]').val(minAmountToPay);
            }
            else if(installmentPayFor =='admission' && isReRegistration =='true'){
             var minAmountToPay = '{!payOptions.Option7.minAmountToPay}';
                $('[id$=InstallmentForAdmission]').prop('checked', true );
                $('[id$=InstallmentAmountValidateForAdmission]').val(minAmountToPay);
                document.getElementById("amountToBePaid").innerHTML = '{!payOptions.Option7.Amount}'-minAmountToPay;                    
                $('[id$=balanceAmount]').show();
                $('[id$=hdnInstallmentAmount]').val(minAmountToPay);
            }           
            else if(installmentPayFor =='AnnualFee'){
               var minAmountToPay = '{!payOptions.Option5.minAmountToPay}';
                $('[id$=InstallmentForAnnual]').prop( 'checked', true );
                $('[id$=InstallmentForCertiAnnual]').prop( 'checked', true );
                 $('[id$=InstallmentAmountValidateForAdmission]').val(minAmountToPay);
                document.getElementById("amountToBePaid").innerHTML = '{!payOptions.Option5.Amount}'-minAmountToPay;                    
                $('[id$=balanceAmount]').show();
                $('[id$=hdnInstallmentAmount]').val(minAmountToPay);
