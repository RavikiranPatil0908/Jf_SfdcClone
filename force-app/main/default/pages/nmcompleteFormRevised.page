                    $(document).ready(function (){
                        //For the OTP Timer.
                    })
                    function fmtMSS(s){return(s-(s%=60))/60+(9<s?':':':0')+s}
                    function otpTimer() {
                                $('.timer').html(timeVal);
                                $('.showTimer').html(fmtMSS(timeVal));
                                clearInterval(loadTimer);
                                loadTimer = setInterval(refreshTimer, timerInterval);
                                refreshTimer();
                            }

                            // Alloted time in seconds
                            var timeVal = 180;
                            $('.showTimer').html(fmtMSS(timeVal));
                            $('.timer').html(timeVal);
                            refreshTimer();

                            // Interval
                            var timerInterval = 1000;
                            var loadTimer = setInterval(refreshTimer, timerInterval);

                            // Refresh Timer
                            function refreshTimer() {
                                setTimeout(function () {
                                    var myTime = parseInt($('.timer').html()) - 1;
                                    if ( myTime > 0) {
                                        $('.timer').html(myTime);
                                        $('.showTimer').html(fmtMSS(myTime));
                                    } else {
                                        $('.timer').html('');
                                        $('.showTimer').html('');
                                        $('#divTimer').addClass('hidden');
                                        // $(".sendBtn").disabled = false
                                        $(".sendBtn").removeClass('disabled');
                                        $(".sendBtn").prop("disabled", false);
                                        // $("[id$=sendBtn]").removeClass('disabled');
                                    }
                                }, 1);
                            }
                        $('[id$=otp]').val('');
                        otpTimer();
                        function sendOTP() {
                            console.log('sendOTP called--> ' );
                            $('[id$=hdnResendCode]').val('true');
                            validateOTPStatus();
                        }
                        function onOtpLogout() {
                            window.location.href = "/nmlogin_new?&type=registration";
                        }
                        function validate() {
                            if ($('[id$=otp]').val()) {
                                // $('.otpPopUp').hide();
                                verifyOtp();
                                $('.loaderPanel').show();
                                return false;
                            }
                            return true;
                        }
