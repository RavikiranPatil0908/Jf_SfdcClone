        <script>
            $(document).ready(function (){
                var _message = 'For this eligibility criteria its mandatory to submit Work experience proof of';
                var AllowNextTab = true;
                if('{!JSENCODE(strSecToShow)}' =='PopUpError' && '{!JSENCODE(message)}'.indexOf(_message) == -1){
                    openPopUp();
                    AllowNextTab = false;
                }

                /*------------------------------------------------After ReLogin check upto which tab details fill based upon that enabled tab -------*/     
                var relogin = '{!IsReLogin}';
                console.log('relogin--->'+relogin );
                if('{!IsReLogin}' == 'true') {
                    validateTabs(false);

                    $('.gotoTabs').click(function( event ) {
                        var parent = $(this).parent();

                        //To differentiate between human and event click
                        if(event.originalEvent != undefined && !parent.hasClass('disabled')) {
                            var currentTab = $('li.active a.gotoTabs').attr('data-tab');
                            var tabNumber = $(this).data('tab');
                            var checkTab = false;
                            
                            if(currentTab > 1 && currentTab != tabNumber) {
                                var checkTabNumber = validateTabs(true);

                                if(checkTabNumber < tabNumber) {
                                    switch (checkTabNumber) {
                                        case 2:
                                            isPreviousPageToPersonalInformationTab();
                                            break;
                                        case 3:
                                            isPreviousPageToAddressInformationTab();
                                            break;
                                        case 4:
                                            isPreviousPageToProgramDetailsTab();
                                            break;
                                        case 5:
                                            isPreviousPageToAcademicInformationTab();
                                            break;
                                        default:
                                            isPreviousPageToWorkExperienceTab();
                                            break;
                                    }
                                    return false;
                                }else{
                                    return true;
                                }
                            }
                        }
                    });
                }

                if('{!sectionName}' == 'Generate Registration No' &&  '{!IsEditFormConfirmation}' != 'true' && AllowNextTab){
                    if(relogin != 'true') {
                        onClickNextToPersonalInformationTab(false);
                    }else{
                        onClickNextToAddressInformationTab(false);
                    }
                }else 
                if('{!sectionName}' == 'Personal Information' && '{!IsEditFormConfirmation}' != 'true' ){
                    if(AllowNextTab) {
                        onClickNextToAddressInformationTab(false);
                    } else {
                        onClickNextToPersonalInformationTab(false);
                    }
                }else if('{!sectionName}' == 'Address Information' && '{!IsEditFormConfirmation}' != 'true'){
                    if(AllowNextTab) {
                        onClickNextToProgramDetailsTab(false);
                    } else {
                        onClickNextToAddressInformationTab(false);
                    }
                }else if('{!sectionName}' == 'Program Details' && '{!IsEditFormConfirmation}' != 'true'){
                    if(AllowNextTab) {
                        onClickNextToAcademicInformationTab(false);
                    } else {
                        onClickNextToProgramDetailsTab(false);
                    }
                }else if('{!sectionName}' == 'Academic Information' && '{!IsEditFormConfirmation}' != 'true'){
                    if(AllowNextTab) {
                        onClickNextToWorkExperienceTab(false);
                    } else {
                        onClickNextToAcademicInformationTab(false);
                    }
                } 
                
                /*--------------------------------click from Confirmation Page-----------------*/
                if('{!sectionName}' == 'Personal Information' && '{!IsEditFormConfirmation}' == 'true'){
                    onClickNextToPersonalInformationTab(false);
                }else if('{!sectionName}' == 'Address Information' && '{!IsEditFormConfirmation}' == 'true'){
                    onClickNextToAddressInformationTab(false);
                }else if('{!sectionName}' == 'Program Details' && '{!IsEditFormConfirmation}' == 'true'){
                    onClickNextToProgramDetailsTab(false);
                }else if('{!sectionName}' == 'Academic Information' && '{!IsEditFormConfirmation}' == 'true'){
                    onClickNextToAcademicInformationTab(false);
                }else if('{!sectionName}' == 'Work Experience' && '{!IsEditFormConfirmation}' == 'true'){
                    onClickNextToWorkExperienceTab(false);
                }
                

                function validateTabs(checkVal) {
                    $('[id$=generateRegistrationNoTab]').addClass('disabled');
                    var _flag9 = validateForm('[id$=personalInformation]',checkVal);
                    if(_flag9 !=0){
                        $('[id$=personalInformationTab]').removeClass('disabled');
                        return 2;
                    }else{
