<apex:page showHeader="false" sidebar="false" standardStylesheets="false" controller="nmRegistration_Controller" cache="false" applyBodyTag="false" docType="html-5.0">
    <head>
        <meta charset="utf-8" />
        <meta http-equiv="X-UA-Compatible" content="IE=edge"/>
        <title>NMIMS Distance Education: Registration Form</title>
        <meta name="viewport" content="width=device-width, initial-scale=1" />
        <meta name="description" content="Award winning distance education university, Narsee Monjee offers wide range of Professional management courses like PG, Diploma & Certification programs."/>
        <meta name="keywords" content="Narsee Monjee Distance Learning University, NMIMS Distance Programs, Executive Education, Top distance learning program in India, PG Diploma, Certification courses, Online Education"/>
        
        <!-- StyleSheets -->
        <apex:stylesheet value="{!URLFOR($Resource.NewResourceRevised, 'NewResourceRevised/assets/css/bootstrap.min.css')}"/>
        <apex:stylesheet value="{!URLFOR($Resource.NewResourceRevised, 'NewResourceRevised/assets/css/bootstrap-datetimepicker.min.css')}"/>
        <apex:stylesheet value="{!URLFOR($Resource.NewResourceRevised, 'NewResourceRevised/assets/css/fonts.css')}"/>
        <apex:stylesheet value="{!URLFOR($Resource.NewResourceRevised, 'NewResourceRevised/assets/css/style.min.css')}"/>
        <apex:stylesheet value="{!URLFOR($Resource.NewResourceRevised, 'NewResourceRevised/assets/css/easy-autocomplete.min.css')}"/>
        <apex:stylesheet value="{!URLFOR($Resource.NewResourceRevised, 'NewResourceRevised/assets/css/easy-autocomplete.themes.min.css')}"/>
        <apex:stylesheet value="https://cdnjs.cloudflare.com/ajax/libs/select2/4.0.10/css/select2.min.css" html-integrity="sha256-FdatTf20PQr/rWg+cAKfl6j4/IY3oohFAJ7gVC3M34E=" html-crossorigin="anonymous" />
        
        <script type="text/javascript">
            history.pushState(null, null, 'corporatecompleteForm');
            window.addEventListener('popstate', function () {
                history.pushState(null, null, 'corporatecompleteForm');
            });
        </script>

        <style>
            .overlay {
                position: fixed;
                top: 0;
                left: 0;
                width: 100%;
                height: 100%;
                z-index: 10;
                background-color: rgba(0,0,0,0.5);
            }
            .poup {
                position: fixed;
                top: 30%;
                width: 40%;
                margin-left: 30%;
                text-align: center;
                z-index: 11;
            }
            .camelcase {
                text-transform:capitalize;
            }
                
            @media (min-width: 1200px) {
                .tab-wrapper .nav-tabs li {
                    width: 16.6%;
                }
            }

            @media (max-width: 480px) {
                .bannerImage {
                    width: 165% !important;
                }
            }

            .errorBorder {
                border:1px solid red !important;
            }

            .svg-inline--fa {
                vertical-align: top;
            }

            span.select2-results > ul.select2-results__options {
                font: normal 1em Calibri-Light !important;
            }
        </style>
    </head>
    <body class="home">
        <!-- Header component -->
        <c:HeaderPage BannerSrc="form_banner.webp" ActiveBadge="Apply Now"/>
        <!-- Header component -->

        <!-- Form Starts Here -->
        <div class="row">
            <apex:outputPanel styleClass="col-sm-12 regWindowdiv" rendered="{!!checkExpireDateforBatch}">
             <center><b><h4 style="color: red;">Registration window is closed as of now...</h4></b></center>   
            </apex:outputPanel>
        </div>
        <br />
        <div class="tab-wrapper">
            <div class="container">
                <ul class="nav nav-tabs">
                    <li class="active list-group" id="generateRegistrationNoTab"><a href="#generateRegistrationNoPanel" class="generateRegistrationNoTab" data-toggle="tab" data-tab="1">
                        STEP 1<span>GENERATE REGISTRATION NO </span>
                    </a></li>
                    <li class="disabled list-group" id="personalInformationTab"><a href="#personalInformationPanel" class="personalInformationTab gotoTabs" data-toggle="tab" data-tab="2">
                        STEP 2<span>PERSONAL INFORMATION</span>
                    </a></li>
                    <li class="disabled list-group" id="addressInformationTab"><a href="#addressInformationPanel" class="addressInformationTab gotoTabs" data-toggle="tab" data-tab="3">
                        STEP 3<span>ADDRESS INFORMATION</span>
                    </a></li>
                    <li class="disabled list-group" id="programDetailsTab"><a href="#programDetailsPanel" class="programDetailsTab gotoTabs" data-toggle="tab" data-tab="4">
                        STEP 4<span>PROGRAM DETAILS</span>
                    </a></li>
                    <li class="disabled list-group" id="academicInformationTab"><a href="#academicInformationPanel" class="academicInformationTab gotoTabs" data-toggle="tab" data-tab="5">
                        STEP 5<span>ACADEMIC INFORMATION</span>
                    </a></li>
                    <li class="disabled list-group" id="workExperienceTab"><a href="#workExperiencePanel" class="workExperienceTab gotoTabs" data-toggle="tab" data-tab="6">
                        STEP 6<span>WORK EXPERIENCE</span>
                    </a></li>
                </ul>
                
                <div class="tab-content">
                    <!----------------------------------------------- Generate Reigistration number Starts Here -------------------------------->
                    <div class="tab-pane fade in active " id="generateRegistrationNoPanel" >
                        <apex:Form id="generateRegistrationNo">
                            <!-- Hidden Field -->
                            <apex:outputPanel id="ErrorPopUpPanel">
                                <apex:inputHidden id="message" value="{!message}"/>
                                <apex:inputHidden id="strSecToShow" value="{!strSecToShow}"/>
                            </apex:outputPanel>
                            
                            <apex:outputPanel >
                                <div class="message-box">PLEASE FILL THE BELOW DETAILS TO GENERATE THE REGISTRATION NUMBER.</div>
                                <div class="form-wrapper">
                                    <apex:actionFunction name="ChooseProgramChange" rerender="chooseProgramPanel"/>
                                    <!-- Part I -->
                                    <div class="row">
                                        <div class="col-md-2">
                                            <div class="form-group required-label">
                                                <div class="select-wrapper">
                                                    <apex:inputField id="fsal" value="{!objLead.Salutation}" styleClass="form-control notnull ModifyFirstOptionForSalutation"/>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="col-md-5">
                                            <div class="form-group required-label">
                                                <apex:inputField id="fnRegtrn" styleclass="form-control notnull onlyAlphabets camelcase" value="{!objLead.FirstName}" html-placeholder="First Name"/>
                                            </div>
                                        </div>
                                                
                                        <div class="col-md-5">
                                            <div class="form-group required-label">
                                                <div class="requiredInput">
                                                    <div class="requiredBlock"></div>
                                                    <apex:inputField id="lnRegtrn" styleclass="form-control notnull onlyAlphabets camelcase" value="{!objLead.LastName}" html-placeholder="Last Name"/>
                                                </div>
                                            </div>
                                        </div> 
                                        
                                        <div class="col-md-6">
                                            <div class="form-group required-label">
                                                <apex:inputText styleClass="form-control notnull trackChangeInValueClass emailValidation" id="emailForRegistratn"  html-placeholder="Email ID" value="{!objLead.Email}" disabled="{!IsReLogin}"/>
                                            </div>
                                        </div>
                                        
                                        <div class="col-md-6">
                                            <div class="form-group required-label">
                                                <apex:inputText styleClass="form-control numberOfDigits mobileNoValidation notnull numonly trackChangeInValueClass" id="mobileForRegistratn"  html-placeholder="Mobile Number" value="{!objLead.MobilePhone}" disabled="{!IsReLogin}"/>
                                            </div>
                                        </div>
                                        
                                        <div class="col-md-6">
                                            <div class="form-group required-label">
                                                <div class="input-group date datetimepicker1 dateOfBirth" data-date-format="DD/MM/YYYY" ><span class="dateInput dateOnlyInput">
                                                <apex:inputfield value="{!objLead.nm_DateOfBirth__c}" styleClass="form-control notnull"  showDatePicker="false" id="DOBForRegistratn" html-placeholder="Date Of Birth"/>
                                                </span><span class="input-group-addon"><span class="glyphicon glyphicon-calendar"></span></span></div>
                                            </div>
                                        </div>
                                        <input type="hidden" id="ageAsOn" />

                                        <apex:inputHidden value="{!LeadSource}" html-class="LeadSourceVal" />
                                        <script>
                                            $('.LeadSourceVal').val('Corporate');
                                        </script>
                                    </div>

                                    <div class="clear-fix"></div>

                                    <!-- Part 2 -->
                                    <div class="row">
                                        <div class="form-wrapper">
                                            <div class="form-group">
                                                <label >Choose a Program</label>
                                                <div class="row">
                                                    <div class="col-md-3">
                                                        <div class="radio">
                                                            <label>
                                                            <input type="radio" value="Certificate Programs" name="programRadio" id="CertificatePrograms" data-mode="1" class="iCheckRadio" />
                                                            Certificate Programs </label>
                                                        </div>
                                                    </div>
                                                    <div class="col-md-3">
                                                        <div class="radio">
                                                            <label>
                                                            <input type="radio" value="Diploma Programs" name="programRadio" id="DiplomaPrograms" data-mode="2" class="iCheckRadio" />
                                                            Diploma Programs </label>
                                                        </div>
                                                    </div>
                                                    <div class="col-md-3">
                                                        <div class="radio">
                                                            <label>
                                                            <input type="radio" value="Post Graduate Diploma Programs" name="programRadio" id="PostGraduateDiplomaPrograms" data-mode="3" class="iCheckRadio" />
                                                            Post Graduate Diploma Programs </label>
                                                        </div>
                                                    </div>

                                                    <div class="col-md-3">
                                                        <div class="radio">
                                                            <label>
                                                            <input type="radio" value="Professional Programs" name="programRadio" id="ProfessionalPrograms" data-mode="4" class="iCheckRadio" />
                                                            Professional Programs </label>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>

                                            <apex:outputPanel id="chooseProgramPanel">
                                                <apex:actionRegion >
                                                    <apex:inputHidden id="hdnChooseProgram1" value="{!objLead.nm_ChooseaProgram__c}" />
                                                    <div class="col-md-6">
                                                        <div class="form-group required-label">
                                                            <div class="select-wrapper">
                                                                <apex:selectList id="Programs" value="{!objLead.nm_Program__c}" styleClass="form-control notnull" multiselect="false" size="1" disabled="{!IsReLogin}">
                                                                    <apex:selectOptions value="{!ProgramsList}"/>
                                                                    <apex:actionSupport event="onchange" rerender="CenterIdForRegitn" />
                                                                </apex:selectList>
                                                            </div>
                                                        </div>
                                                    </div>
                                                     
                                                    <div class="col-md-6">
                                                        <div class="form-group required-label">
                                                            <div class="select-wrapper">
                                                                <apex:selectList value="{!objLead.nm_InformationCenter__c}" styleClass="form-control notnull selectedCenter" multiselect="false" size="1" style="width:99%;" disabled="{!IsReLogin}">
                                                                    <apex:selectOption itemValue="a029000000gDLiaAAG" itemLabel="Hyderabad - Gachibowli"/>
                                                                </apex:selectList>  
                                                            </div>
                                                        </div>
                                                    </div>
                                                </apex:actionRegion>    
                                            </apex:outputPanel>
                                        </div>
                                    </div>
                                    
                                    <!-- Part 3 -->
                                    <div class="row" >
                                        <apex:outputPanel styleClass="col-sm-6 saveProceedBtn" rendered="{!checkExpireDateforBatch}">
                                            <apex:commandLink styleClass="btn pgSubmit " id="generateRegistrationNoButton"  value="Next" action="{!nextPage}" onclick="return gotoNextPage();" >
                                                <apex:param name="section"  value="Generate Registration No" assignTo="{!sectionName}"/>
                                            </apex:commandLink> 
                                        </apex:outputPanel> 
                                    </div>
                                </div>
                            </apex:outputPanel>
                        </apex:Form>

                    </div>
                    <!----------------------------------------------- Generate Reigistration number Ends Here -------------------------------->
                    
                    <!----------------------------------------------- Personal Information Starts Here -------------------------------->
                    <div class="tab-pane fade " id="personalInformationPanel" >
                        <apex:form id="personalInformation">
                            <div class="message-box">Your Name will appear as below in All University Documents. Please change First Name and Last Name if you wish to.<br/> <span class="fullNamePreview"></span></div>
                            <div class="form-wrapper">
                                <div class="row">

                                    <div class="col-md-2">
                                        <div class="form-group required-label">
                                            <div class="select-wrapper">
                                                <apex:inputField id="fnsal" value="{!objLead.Salutation}" styleClass="form-control notnull ModifyFirstOptionForSalutation"/>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="col-md-5">
                                        <div class="form-group required-label">
                                            <apex:inputField id="fn1" styleclass="form-control onlyAlphabets notnull prospectName" value="{!objLead.FirstName}" html-placeholder="First Name"/>
                                            <p class="note"><span style="color:red;">*</span> Enter as per final mark-sheet:</p>
                                        </div>
                                    </div>

                                    <div class="col-md-5">
                                        <div class="form-group required-label">
                                            <div class="requiredInput">
                                                <div class="requiredBlock"></div>
                                                <apex:inputField id="ln1" styleclass="form-control onlyAlphabets notnull prospectName" value="{!objLead.LastName}" html-placeholder="Last Name"/>
                                                <p class="note"><span style="color:red;">*</span> Enter as per final mark-sheet:</p>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="col-md-2">
                                        <div class="form-group required-label">
                                            <div class="select-wrapper">
                                                <apex:inputField id="fnsalF" value="{!objLead.Father_Salutation__c}" styleClass="form-control notnull ModifyFirstOptionForSalutation"/>
                                            </div>
                                        </div>
                                    </div>
            
                                    <div class="col-md-5">
                                        <div class="form-group required-label">
                                        <apex:inputField styleClass="form-control onlyAlphabets notnull" html-placeholder="Fathers First Name" id="fname"  value="{!objLead.Father_First_Name__c}"/>
                                        </div>
                                    </div>
                                    
                                    <div class="col-md-5">
                                        <div class="form-group required-label">
                                        <apex:inputField styleClass="form-control onlyAlphabets notnull" html-placeholder="Fathers Last Name" id="fLname"  value="{!objLead.Father_Last_Name__c}"/>
                                        </div>
                                    </div>
                                    
                                    <div class="col-md-2">
                                        <div class="form-group required-label">
                                            <div class="select-wrapper">
                                                <apex:inputField id="fnsalM" value="{!objLead.Mother_Salutation__c}" styleClass="form-control notnull ModifyFirstOptionForSalutation"/>
                                            </div>
                                        </div>
                                    </div>
            
                                    <div class="col-md-5">
                                        <div class="form-group required-label">
                                            <apex:inputField styleClass="form-control onlyAlphabets notnull" id="mname" html-placeholder="Mothers First Name" value="{!objLead.Mother_First_Name__c}"/>
                                        </div>
                                    </div>
                                    
                                    <div class="col-md-5">
                                        <div class="form-group required-label">
                                            <apex:inputField styleClass="form-control onlyAlphabets notnull" id="mLname" html-placeholder="Mothers Last Name" value="{!objLead.Mother_Last_Name__c}"/>
                                        </div>
                                    </div>
                                    
                                    <div class="col-md-6">
                                        <div class="form-group required-label">
                                            <div class="input-group date datetimepicker1 dateOfBirth dateOfBirth12" data-date-format="DD/MM/YYYY" ><span class="dateInput dateOnlyInput">
                                            <apex:inputfield value="{!objLead.nm_DateOfBirth__c}" styleClass="form-control notnull"  showDatePicker="false" id="dateOfBirth" html-placeholder="Date Of Birth"/>
                                            </span><span class="input-group-addon"><span class="glyphicon glyphicon-calendar"></span></span></div>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-group required-label">
                                            <div class="select-wrapper">
                                                <apex:inputField id="gender" value="{!objLead.nm_Gender__c}" styleClass="form-control notnull ModifyFirstOptionForGender"/>
                                            </div> 
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-group required-label">
                                            <div class="select-wrapper">
                                                <apex:inputField id="mstatus"  value="{!objLead.nm_MaritalStatus__c}" styleClass="form-control notnull ModifyFirstOptionForMaritalStatus"/>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <apex:inputField styleClass="form-control" id="spouse" html-placeholder="Spouse Name" value="{!objLead.nm_SpouseName__c}"/>
                                        </div>
                                    </div>

                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <apex:inputField value="{!objLead.Adhar_Card_Number__c}" styleclass="form-control  numonly" id="aadharCard"  html-placeholder="Aadhar Number"/>
                                        </div>
                                    </div>

                                    <div class="col-md-6" id="pageId:formid1:idBloodGroup">
                                        <div class="form-group required-label">
                                            <div class="select-wrapper">
                                                <apex:inputField id="bldgrp" value="{!objLead.nm_BloodGroup__c}" styleClass="form-control notnull ModifyFirstOptionBloodGroup"/>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-6" id="pageId:formid1:idCategory">
                                        <div class="form-group required-label">
                                            <div class="select-wrapper">
                                                <apex:inputField id="ctgry" value="{!objLead.nm_Category__c}" styleClass="form-control notnull ModifyFirstOptionCategory"/>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-group required-label">
                                            <div class="select-wrapper">
                                                <apex:inputField id="ntly" value="{!objLead.nm_Nationality__c}" styleClass="form-control notnull ModifyFirstOptionNationality "/>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-group required-label">
                                            <div class="select-wrapper">
                                                <apex:selectList id="domicile" value="{!objLead.nm_Domicile__c}" styleClass="form-control notnull ModifyFirstOptionDomicile" size="1" multiselect="false" style="width:99%;" >
                                                    <apex:selectOptions value="{!domicileList}"/>
                                                </apex:selectList>
                                            </div> 
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-group required-label">
                                            <div class="select-wrapper">
                                                <apex:inputField id="motherTongue" value="{!objLead.nm_Mother_Tongue__c}" styleClass="form-control notnull ModifyFirstOptionMotherTongue "/>
                                            </div> 
                                        </div>
                                    </div>
                                    <div class="col-md-6 otherMotherTonguePanel" style="display:none;">
                                        <div class="form-group required-label">
                                            <apex:inputField id="otherMotherTongueId" value="{!objLead.nm_Other_Mother_Tongue__c}" styleClass="form-control " html-placeholder="If Mother Tongue Others, please specify:"/>
                                        </div>
                                    </div>
                                </div>
                                <apex:outputPanel rendered="{!checkExpireDateforBatch}">
                                    <div class="row">
                                        <div class="col-sm-6 text-left">
                                            <apex:commandLink styleClass="btn pgSubmit " id="savePersonalDetails" onclick="return gotoAddressInformation();"  value="Next" action="{!nextPage}" rendered="{!!IsEditFormConfirmation}">
                                                <apex:param name="section"  value="Personal Information" assignTo="{!sectionName}"/>
                                            </apex:commandLink>
                                            <apex:commandButton styleClass="btn pgSubmit " onclick="return onSubmit('personalInformation');" id="goToConfirmation1" value="SubmitDetails" action="{!GoToConfirmationForm}" rendered="{!IsEditFormConfirmation}" />
                                        </div>
                                    </div>
                                </apex:outputPanel>
                            </div>
                        </apex:form>
                    </div>
                    <!----------------------------------------------- Personal Information Ends Here -------------------------------->
                    
                    <!----------------------------------------------- Address Information Starts Here -------------------------------->
                    <div class="tab-pane fade " id="addressInformationPanel" >
                        <apex:Form id="addressInformation">
                            <!------------------------------------- Hidden Fields -------------------------------------------->
                            <apex:inputHidden id="hdnpostalCode" value="{!PostalCode}"/>
                            <apex:inputHidden id="hdnPermenentAddressChoice" value="{!CopyPermanantAddressFromTo}"/>
                            <apex:inputhidden id="hdnAddressChoice" value="{!objLead.Shipping_Address_Choice__c}" /> 

                            <apex:actionFunction name="getShipingCountryStateCityFromPostalAPI" action="{!getCountryStateCityFromPostalAPI}" reRender="txtSCity,txtSState,txtSCountry,postalCodeMessage" oncomplete="postalCodeStatus('sAddress');showShippingAddressPreview();"/>
                            <apex:actionFunction name="getPermanentCountryStateCityFromPostalAPI" action="{!getCountryStateCityFromPostalAPI}" reRender="txtPCity,txtPState,txtPCountry,postalCodeMessage" oncomplete="postalCodeStatus('perAddress');"/>
                            <apex:inputHidden value="{!PostalCodeMessage}" id="postalCodeMessage" />

                            <div class="form-wrapper" style="padding-top:0">
                                <div class="message-box text-center">Shipping Address</div>
                                <br></br>
                                <label for="ShippingAddress">Your study kit,ID Card and Fee Receipt will be shipped on the below Address</label>
                                <div class="row">
                                    <div class="col-md-4">
                                        <div class="form-group required-label">
                                            <apex:inputField styleClass="form-control notnull numonly trackChangeInValueClass PincodeLength sAddress addressPreview"  id="txtSPostalCode"  html-placeholder="Pin Code" value="{!objLead.Zip_Postal_Code_Shipping__c}"  />
                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="form-group required-label">
                                            <apex:inputField styleclass="form-control notnull trackChangeInValueClass disabled sAddress addressPreview" id="txtSCity"  html-placeholder="City" value="{!objLead.Shipping_City__c}" html-disabled="true" />
                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="form-group required-label">
                                            <div class="select-wrapper">
                                                <apex:inputField id="txtSState"  value="{!objLead.State_Province_Shipping__c}" styleClass="form-control notnull trackChangeInValueClass sAddress addressPreview" html-disabled="true" html-placeholder="State"/>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="form-group required-label">
                                            <apex:inputField styleClass="form-control notnull trackChangeInValueClass addressPreview"  id="txtSHouseNumber" html-placeholder="Address line 1"  value="{!objLead.House_No_Name_Shipping__c}" html-maxlength="30"/>
                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="form-group required-label">
                                            <apex:inputField id="txtSStreet" value="{!objLead.Shipping_Street__c}" styleClass="form-control notnull trackChangeInValueClass addressPreview" html-placeholder="Address line 2" html-maxlength="30"/>
                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="form-group">
                                            <apex:inputField styleClass="form-control trackChangeInValueClass addressPreview"  id="txtSLocality" html-placeholder="Address line 3" value="{!objLead.Locality_Name_Shipping__c}" html-maxlength="255"/>
                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="form-group required-label">
                                            <div class="select-wrapper">
                                                <apex:inputField id="txtSCountry"  value="{!objLead.Country_Shipping__c}" styleClass="form-control notnull sAddress trackChangeInValueClass addressPreview" html-disabled="true" html-placeholder="Country"/>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="form-group required-label">
                                            <apex:inputField styleClass="form-control notnull trackChangeInValueClass addressPreview" id="txtSLandmark" html-placeholder="Nearby Landmark" value="{!objLead.Nearest_LandMark_Shipping__c}"/>
                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="form-group">
                                            <textarea cols="80" readonly="readonly" id="AddressPreview" maxlength="500"  row="12" placeholder="Shipping Address" class="form-control notnull" style="width: 340px; height: 60px"></textarea>
                                        </div>
                                    </div>
                                    <div class="col-xs-4 col-md-4">
                                        <div class="form-group">
                                            <apex:inputField styleClass="form-control numonly"  id="txtBStdCode" html-placeholder="STD" value="{!objLead.nm_STDCode__c}"/>
                                        </div>
                                    </div>
                                    <div class="col-xs-8 col-md-4">
