<apex:component access="global" allowDML="true" controller="PaymentPageController">
    <apex:attribute access="global" name="recordId" type="ID" description="Record Id" assignTo="{!objectId}"/>
    <apex:attribute access="global" name="objType" type="String" description="Object Type" assignTo="{!objectType}"/>
    <!-- Custom Css -->
    <link href="{!URLFOR($Resource.AdminBSBMaterialDesign, 'AdminBSBMaterialDesign/css/payment.css')}" rel="stylesheet"/>
    <!-- Bootstrap DatePicker Css -->
    <link href="{!URLFOR($Resource.AdminBSBMaterialDesign, 'AdminBSBMaterialDesign/css/bootstrap-datetimepicker.min.css')}" rel="stylesheet" />

    <div class="container-fluid">
        <div class="row clearfix">
            <apex:outputPanel >
                <apex:inputHidden id="hdngetWay" value="{!paymentGateWayName}" />
                <apex:actionFunction name="callMakePayment" action="{!makeOnlinePayment}" />
                <apex:inputHidden id="hdnPaymentMode" value="{!objPayment.nm_ModeOfPayment__c}" />
                <apex:inputHidden id="hdnPaymentOption" value="{!paymentFor}" />
                <apex:inputHidden id="hdnLiveLecturesOption" value="{!objDetails.liveLecType}" />
                <apex:inputHidden id="hdnCourseList" value="{!objDetails.courseList}" />
                <apex:inputHidden id="hdnBacklogList" value="{!objDetails.backlogList}" />
                <apex:inputHidden id="hdnDepositeAmount" value="{!depositAmount}" />
                <apex:inputHidden id="hdnTenure" value="{!objLoanDetails.Tenure}" />
                <apex:inputHidden id="hdnEMI" value="{!objLoanDetails.EMI}" />
                <apex:inputHidden id="hdnInterestrate" value="{!objLoanDetails.InterestRate}" />
                <apex:inputHidden id="hdnLoanType" value="{!objLoanDetails.LoanType}" />
                <apex:inputHidden value="{!objDetails.applicableCareerServiceFee}" id="applicableCareerServiceFee" />
                <apex:inputHidden value="{!objCoursera.applicableCourseraFee}" id="applicableCourseraFee"/>
                <apex:inputHidden id="hdnSubjectsCount" value="{!jsonMapOfLectureTypeAndSubjects}" />
                <apex:inputHidden id="checkCareerServiceOpted" value="{!objDetails.CareerServiceOpted}" />
                <apex:inputHidden id="checkCourseraOpted" value="{!objCoursera.checkCourseraOpted}" />
                <apex:inputHidden value="{!jsonMapOfProductAndAmount}" id="jsonStringOfProductVsAmount" />
            </apex:outputPanel>
            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                <div class="card">
                    <div class="body">
                        <h2 class="card-inside-title">Your Details</h2>
                        <div class="row studentDetails">
                            <div class="row col-xs-12 col-sm-12 col-md-6 col-lg-6 m-b-0">
                                <div class="col-xs-6 col-sm-6 col-md-6 col-lg-6"><span class="table-label">Registration ID</span></div>
                                <div class="col-xs-6 col-sm-6 col-md-6 col-lg-6"><span>{!objStudentDetails.studentNo}</span></div>
                            </div>
                            <div class="row col-xs-12 col-sm-12 col-md-6 col-lg-6 m-b-0">
                                <div class="col-xs-6 col-sm-6 col-md-6 col-lg-6"><span class="table-label">Applicant Name</span></div>
                                <div class="col-xs-6 col-sm-6 col-md-6 col-lg-6"><span>{!objStudentDetails.studentName}</span></div>
                            </div>
                            <div class="row col-xs-12 col-sm-12 col-md-6 col-lg-6 m-b-0">
                                <div class="col-xs-6 col-sm-6 col-md-6 col-lg-6"><span class="table-label">Program Category</span></div>
                                <div class="col-xs-6 col-sm-6 col-md-6 col-lg-6"><span>{!programCategory}</span></div>
                            </div>
                            <apex:outputPanel rendered="{!!ISBLANK(objDetails.program)}" layout="none">
                                <div class="row col-xs-12 col-sm-12 col-md-6 col-lg-6 m-b-0">
                                    <div class="col-xs-6 col-sm-6 col-md-6 col-lg-6"><span class="table-label">Program Name</span></div>
                                    <div class="col-xs-6 col-sm-6 col-md-6 col-lg-6"><span>{!objDetails.program}</span></div>
                                </div>
                            </apex:outputPanel>
                            <apex:outputPanel rendered="{!!ISBLANK(objDetails.program)}" layout="none">
                                <div class="row col-xs-12 col-sm-12 col-md-6 col-lg-6 m-b-0">
                                    <div class="col-xs-6 col-sm-6 col-md-6 col-lg-6"><span class="table-label">Program Name</span></div>
                                    <div class="col-xs-6 col-sm-6 col-md-6 col-lg-6"><span>{!objDetails.program}</span></div>
                                </div>
                            </apex:outputPanel>
                            <apex:outputPanel rendered="{!IF(ISBLANK(objDetails.program) && ISBLANK(objDetails.programSpecialisaton),true,false)}" layout="none">
                                <div class="row col-xs-12 col-sm-12 col-md-6 col-lg-6 m-b-0">
                                    <div class="col-xs-6 col-sm-6 col-md-6 col-lg-6"><span class="table-label">Program Name</span></div>
                                    <div class="col-xs-6 col-sm-6 col-md-6 col-lg-6"><span>{!objDetails.programName}</span></div>
                                </div>
                            </apex:outputPanel>
                            <apex:outputPanel rendered="{!!ISBLANK(objDetails.programSpecialisaton)}" layout="none">
                                <div class="row col-xs-12 col-sm-12 col-md-6 col-lg-6 m-b-0">
                                    <div class="col-xs-6 col-sm-6 col-md-6 col-lg-6"><span class="table-label">Specialisation</span></div>
                                    <div class="col-xs-6 col-sm-6 col-md-6 col-lg-6"><span>{!objDetails.programSpecialisaton}</span></div>
                                </div>
                            </apex:outputPanel>
                            <apex:outputPanel layout="none" rendered="{!objDetails.isPaymentMade}">
                                <div class="row col-xs-12 col-sm-12 col-md-12 col-lg-12 m-b-0">
                                    <apex:outputPanel styleClass="col-xs-6 col-sm-6 col-md-6 col-lg-6" rendered="{!IF(!objLoanDetails.isDownPaymentMade || objAdmissionAmount.isRedirect, true, false)}">
                                        <a href="javascript:void(0);" class="btn btn-primary btn-lg btn-custom waves-effect toggle-download"><apex:outputText value="{!IF(objAdmissionAmount.isRedirect, 'Make Payment','Download Transaction Advise')}"></apex:outputText></a>
                                    </apex:outputPanel>
                                    <apex:outputPanel styleClass="col-xs-6 col-sm-6 col-md-6 col-lg-6" layout="block" rendered="{!IF(objLoanDetails.isLoanOpted && !objLoanDetails.loanSanctioned, true, false)}">
                                        <a href="{!$Label.Site_Url}PostPaymentPage?ResTrackId={!objLoanDetails.loanPaymentTrackId}" class="btn btn-primary btn-lg btn-custom waves-effect">View Loan Details</a>
                                    </apex:outputPanel>
                                </div>
                            </apex:outputPanel>
                        </div>
                    </div>
                </div>
            </div>

            <!-- to render if form is submited -->
            <apex:outputPanel rendered="{!isFormSubmitted}" layout="none">
                <apex:outputPanel rendered="{!objDetails.isPaymentMade}" layout="none">
                    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12 payment-table hide" >
                        <div class="card">
                            <div class="header p-b-0 b-b-n p-t-0 p-l-0 p-r-0">
                                <ul class="nav nav-pills nav-justified thumbnail setup-panel align-center bg-steel-blue">
                                    <li class="active">
                                        <a href="javascript:void(0);" style="color: #fff;cursor:default;">
                                            <span class="list-group-item-heading font-18 font-bold">Payment Details</span>
                                        </a>
                                    </li>                     
                                </ul>
                            </div>
                            <div class="body">
                                <div class="table-responsive">
                                    <table class="table table-striped">
                                        <thead>
                                            <tr>
                                                <th>#</th>
                                                <th>REFERNCE NUMBER</th>
                                                <th>TRANSACTION DATE</th>
                                                <th>AMOUNT</th>
                                                <th>DOWNLOAD</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <apex:actionRegion >
                                                <apex:variable var="c" value="{!1}" />
                                                <apex:repeat value="{!objDetails.mapOfPaymentsMade}" var="i">
                                                    <tr>
                                                        <th scope="row">{!c}</th>
                                                        <td>{!objDetails.mapOfPaymentsMade[i].nm_Merchant_Track_Id__c}</td>
                                                        <td>
                                                            <apex:outputText value="{0, date, MMMM d','  yyyy}">
                                                                <apex:param value="{!objDetails.mapOfPaymentsMade[i].nm_TransactionDate__c}" /> 
                                                            </apex:outputText>
                                                        </td>
                                                        <td>&#8377; {!objDetails.mapOfPaymentsMade[i].nm_ActualPaymentAmmount__c}</td>
                                                        <td>
                                                            <a href="{!$Label.Site_Url}{!IF(objDetails.mapOfPaymentsMade[i].nm_PaymentType__c=='Registration','nmThankYou','nmRegistrationThankYou')}?PaymentId={!objDetails.mapOfPaymentsMade[i].Id}" class="btn btn-primary waves-effect waves-light btn-custom" target="_blank">
                                                                <i class="material-icons">file_download</i>
                                                                <span>Transaction Advise</span>
                                                            </a>
                                                        </td>
                                                    </tr>
                                                    <apex:variable var="c" value="{!c+1}" />
                                                </apex:repeat>
                                            </apex:actionRegion>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </apex:outputPanel>

                <apex:outputPanel rendered="{!IF(!objLoanDetails.isDownPaymentMade || objAdmissionAmount.isRedirect, true, false)}" layout="none">
                    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12 payment-detail">
                        <!------------------------------------------------ Fee Type -------------------------------------------------->
                        <div class="card" id="paymentOptionSection">
                            <div class="header p-b-0 b-b-n p-t-0 p-l-0 p-r-0">
                                <ul class="nav nav-pills nav-justified thumbnail setup-panel align-center bg-steel-blue">
                                    <li class="active" id="FeeTypeTab">
                                        <a href="javascript:void(0);" style="color: #fff;cursor:default;">
                                            <span class="list-group-item-heading font-18 font-bold">Select Payment Category</span>
                                        </a>
                                    </li>                     
                                </ul>
                            </div>
                            <div class="body">
                                <div class="row clearfix" >
                                    <div class="col-xs-12 col-sm-12 col-md-8 col-lg-8 br-b-2">
                                        <!-------------------------------------------------------------- Payment Fee Type Section -------------------------------->
                                        <div class="text-align font-bold p-l-15 p-r-15 m-b-10 col-philippine-silver">
                                            Fee Payment Type
                                        </div>
                                        <div class="row clearfix paymentSection">
                                            <apex:variable var="setOfApplicablePaymentOption" value="{!''}{!setOfApplicablePaymentOptionForStudent}" />
                                            <apex:variable var="setOfPaymentOption" value="{!''}{!setOfPaymentOptionsAvailable}" />

                                            <apex:outputpanel rendered="{!CONTAINS(setOfApplicablePaymentOption,'Registration Fee') && !objfee.AlreadyPaidRegistrationFee}" 
                                                styleClass="col-xs-12 col-sm-12 col-md-12 col-lg-12" layout="block">
                                                <div class="form-group form-group-lg m-b-0 rm-m-b">
                                                    <div class="" >
                                                        <input name="group1" type="radio" class="with-gap form-control" id="radio_1"  data-mode="1" data-value="Registration" data-label="Registration {!IF(objDetails.MergeAdmissionFeeWithRegistration,' + Initial amount','')} {!IF(objDetails.CareerServiceOpted,' + Career Services','')} {!IF(objCoursera.checkCourseraOpted,' + Coursera','')}" data-amount="{!( mapOfProductAndAmount['Registration Fee']  +objDetails.applicableCareerServiceFee+objCoursera.applicableCourseraFee+ IF(objDetails.MergeAdmissionFeeWithRegistration, mapOfProductAndAmount['Initial Payment'],0) )}"/>
                                                        <label for="radio_1">Registration {!IF(objDetails.MergeAdmissionFeeWithRegistration,' + Initial amount','')} {!IF(objDetails.CareerServiceOpted,' + Career Services','')} {!IF(objCoursera.checkCourseraOpted,' + Coursera','')} (INR {!( mapOfProductAndAmount['Registration Fee']  +objDetails.applicableCareerServiceFee+objCoursera.applicableCourseraFee+ IF(objDetails.MergeAdmissionFeeWithRegistration, mapOfProductAndAmount['Initial Payment'],0) )})</label>
                                                    </div>
                                                </div>
                                            </apex:outputpanel>
            
                                            <apex:outputpanel rendered="{!CONTAINS(setOfApplicablePaymentOption,'InitialAdmission') && !objfee.AlreadyPaidInitialAdmissionfee && objDetails.ShowInitialAdmissionPayment}"
                                                styleClass="col-xs-12 col-sm-12 col-md-12 col-lg-12" layout="block">
                                                <div class="form-group form-group-lg m-b-0 rm-m-b">
                                                    <div class="" >
                                                        <input name="group1" type="radio" class="with-gap form-control" id="radio_2"  data-mode="1" data-value="InitialAdmissionFee" data-label="{!IF(objfee.AlreadyPaidRegistrationFee,'','Registration + ')} Initial Admission Payment {!IF(objDetails.CareerServiceOpted,' + Career Services','')} {!IF(objCoursera.checkCourseraOpted,' + Coursera','')} {!IF(objDetails.applicableLateFee > 0,' + Late Fee' ,'')}"/>
                                                        <label for="radio_2">{!IF(objfee.AlreadyPaidRegistrationFee,'','Registration + ')} Initial Admission Payment {!IF(objDetails.CareerServiceOpted,' + Career Services','')} {!IF(objCoursera.checkCourseraOpted,' + Coursera','')} {!IF(objDetails.applicableLateFee > 0,' + Late Fee' ,'')} (INR {!(mapOfProductAndAmount['InitialAdmissionFee'] +objDetails.applicableCareerServiceFee+ objCoursera.applicableCourseraFee+ objDetails.applicableLateFee +IF(objfee.AlreadyPaidRegistrationFee,0,mapOfProductAndAmount['Registration Fee']) )})</label>
                                                    </div>
                                                </div>
                                            </apex:outputpanel>
            
                                            <apex:outputpanel rendered="{!CONTAINS(setOfPaymentOption,'Admission')}" styleClass="col-xs-12 col-sm-12 col-md-12 col-lg-12" layout="block">
                                                <div class="form-group form-group-lg m-b-0 rm-m-b">
                                                    <div class="" >
                                                        <input name="group1" type="radio" class="with-gap form-control" id="radio_3"  data-mode="1" data-value="Admission" data-label="{!IF(objfee.AlreadyPaidRegistrationFee,'','Registration + ')} Admission {!IF(objDetails.CareerServiceOpted,' + Career Services','')} {!IF(objCoursera.checkCourseraOpted,' + Coursera','')} {!IF(objDetails.applicableLateFee > 0,' + Late Fee' ,'')} {!IF(CONTAINS(objDetails.ICName,'Verizon'),'+Exam Fee','')}"/>
                                                        <label for="radio_3">{!IF(objfee.AlreadyPaidRegistrationFee,'','Registration + ')} Admission {!IF(objDetails.CareerServiceOpted,' + Career Services','')} {!IF(objCoursera.checkCourseraOpted,' + Coursera','')} {!IF(objDetails.applicableLateFee > 0,' + Late Fee' ,'')} {!IF(CONTAINS(objDetails.ICName,'Verizon'),'+Exam Fee','')} (INR {!(mapOfProductAndAmount['Admission'] + objDetails.applicableCareerServiceFee+ objCoursera.applicableCourseraFee+ objDetails.applicableLateFee + IF(objfee.AlreadyPaidRegistrationFee,0,mapOfProductAndAmount['Registration Fee']))})</label>
                                                    </div>
                                                </div>
                                            </apex:outputpanel>
            
                                            <apex:outputpanel rendered="{!CONTAINS(setOfPaymentOption,'AnnualFee')}" styleClass="col-xs-12 col-sm-12 col-md-12 col-lg-12" layout="block">
                                                <div class="form-group form-group-lg m-b-0 rm-m-b">
                                                    <div class="" >
                                                        <input name="group1" type="radio" class="with-gap form-control" id="radio_4"  data-mode="1" data-value="AnnualFee" data-label="{!IF(objfee.AlreadyPaidRegistrationFee,'','Registration + ')} AnnualFee {!IF(objDetails.CareerServiceOpted,' + Career Services','')} {!IF(objCoursera.checkCourseraOpted,' + Coursera','')} {!IF(objDetails.applicableLateFee > 0,' + Late Fee' ,'')}"/>
                                                        <label for="radio_4">{!IF(objfee.AlreadyPaidRegistrationFee,'','Registration + ')} AnnualFee {!IF(objDetails.CareerServiceOpted,' + Career Services','')} {!IF(objCoursera.checkCourseraOpted,' + Coursera','')} {!IF(objDetails.applicableLateFee > 0,' + Late Fee' ,'')} (INR {!mapOfProductAndAmount['AnnualFee'] + objDetails.applicableCareerServiceFee+ objCoursera.applicableCourseraFee+ objDetails.applicableLateFee + IF(objfee.AlreadyPaidRegistrationFee,0,mapOfProductAndAmount['Registration Fee']) })</label>
                                                    </div>
                                                </div>
                                            </apex:outputpanel>

                                            <apex:outputpanel rendered="{!CONTAINS(setOfPaymentOption,'AnnualFeeLoan')}" styleClass="col-xs-12 col-sm-12 col-md-12 col-lg-12" layout="block">
                                                <div class="form-group form-group-lg m-b-0 rm-m-b">
                                                    <div class="" >
                                                        <input name="group1" type="radio" class="with-gap form-control" id="radio_5"  data-mode="1" data-value="AnnualFeeLoan" data-label="{!IF(objfee.AlreadyPaidRegistrationFee,'','Registration + ')} AnnualFeeLoan {!IF(objDetails.CareerServiceOpted,' + Career Services','')} {!IF(objCoursera.checkCourseraOpted,' + Coursera','')} {!IF(objDetails.applicableLateFee > 0,' + Late Fee' ,'')}"/>
                                                        <label for="radio_5">{!IF(objfee.AlreadyPaidRegistrationFee,'','Registration + ')} AnnualFeeLoan {!IF(objDetails.CareerServiceOpted,' + Career Services','')} {!IF(objCoursera.checkCourseraOpted,' + Coursera','')} {!IF(objDetails.applicableLateFee > 0,' + Late Fee' ,'')} (INR {! mapOfProductAndAmount['AnnualFeeLoan'] + objDetails.applicableCareerServiceFee + objCoursera.applicableCourseraFee+ objDetails.applicableLateFee + IF(objfee.AlreadyPaidRegistrationFee,0,mapOfProductAndAmount['Registration Fee']) })</label>
                                                    </div>
                                                </div>
                                            </apex:outputpanel>
            
                                            <apex:outputpanel rendered="{!CONTAINS(setOfPaymentOption,'2TermFee')}" styleClass="col-xs-12 col-sm-12 col-md-12 col-lg-12" layout="block">
                                                <div class="form-group form-group-lg m-b-0 rm-m-b">
                                                    <div class="" >
                                                        <input name="group1" type="radio" class="with-gap form-control" id="radio_6"  data-mode="1" data-value="2TermFee" data-label="{!IF(objfee.AlreadyPaidRegistrationFee,'','Registration + ')} 2TermFee {!IF(objDetails.CareerServiceOpted,' + Career Services','')} {!IF(objCoursera.checkCourseraOpted,' + Coursera','')} {!IF(objDetails.applicableLateFee > 0,' + Late Fee' ,'')}"/>
                                                        <label for="radio_6">{!IF(objfee.AlreadyPaidRegistrationFee,'','Registration + ')} 2TermFee {!IF(objDetails.CareerServiceOpted,' + Career Services','')} {!IF(objCoursera.checkCourseraOpted,' + Coursera','')} {!IF(objDetails.applicableLateFee > 0,' + Late Fee' ,'')} (INR {!mapOfProductAndAmount['2TermFee'] + objDetails.applicableCareerServiceFee+ objCoursera.applicableCourseraFee+ objDetails.applicableLateFee + IF(objfee.AlreadyPaidRegistrationFee,0,mapOfProductAndAmount['Registration Fee']) })</label>
                                                    </div>
                                                </div>
                                            </apex:outputpanel>
            
                                            <apex:outputpanel rendered="{!CONTAINS(setOfPaymentOption,'3SemFee')}" styleClass="col-xs-12 col-sm-12 col-md-12 col-lg-12" layout="block">
                                                <div class="form-group form-group-lg m-b-0 rm-m-b">
                                                    <div class="" >
                                                        <input name="group1" type="radio" class="with-gap form-control" id="radio_7"  data-mode="1" data-value="3SemFee" data-label="{!IF(objfee.AlreadyPaidRegistrationFee,'','Registration + ')} {!IF(objDetails.isTerm,'3TermFee','3SemFee')} {!IF(objDetails.CareerServiceOpted,' + Career Services','')} {!IF(objCoursera.checkCourseraOpted,' + Coursera','')} {!IF(objDetails.applicableLateFee > 0,' + Late Fee' ,'')}"/>
                                                        <label for="radio_7">{!IF(objfee.AlreadyPaidRegistrationFee,'','Registration + ')} {!IF(objDetails.isTerm,'3TermFee','3SemFee')} {!IF(objDetails.CareerServiceOpted,' + Career Services','')} {!IF(objCoursera.checkCourseraOpted,' + Coursera','')} {!IF(objDetails.applicableLateFee > 0,' + Late Fee' ,'')} (INR {! mapOfProductAndAmount['3SemFee'] + objDetails.applicableCareerServiceFee + objCoursera.applicableCourseraFee+ objDetails.applicableLateFee + IF(objfee.AlreadyPaidRegistrationFee,0,mapOfProductAndAmount['Registration Fee']) })</label>
                                                    </div>
                                                </div>
                                            </apex:outputpanel>
            
                                            <apex:outputpanel rendered="{!CONTAINS(setOfPaymentOption,'4SemFee')}" styleClass="col-xs-12 col-sm-12 col-md-12 col-lg-12" layout="block">
                                                <div class="form-group form-group-lg m-b-0 rm-m-b">
                                                    <div class="" >
                                                        <input name="group1" type="radio" class="with-gap form-control" id="radio_8"  data-mode="1" data-value="4SemFee" data-label="{!IF(objfee.AlreadyPaidRegistrationFee,'','Registration + ')} {!IF(objDetails.isTerm,'4TermFee','4SemFee')} {!IF(objDetails.CareerServiceOpted,' + Career Services','')} {!IF(objCoursera.checkCourseraOpted,' + Coursera','')} {!IF(objDetails.applicableLateFee > 0,' + Late Fee' ,'')}"/>
                                                        <label for="radio_8">{!IF(objfee.AlreadyPaidRegistrationFee,'','Registration + ')} {!IF(objDetails.isTerm,'4TermFee','4SemFee')} {!IF(objDetails.CareerServiceOpted,' + Career Services','')} {!IF(objCoursera.checkCourseraOpted,' + Coursera','')} {!IF(objDetails.applicableLateFee > 0,' + Late Fee' ,'')} (INR {! mapOfProductAndAmount['4SemFee'] + objDetails.applicableCareerServiceFee + objCoursera.applicableCourseraFee+ objDetails.applicableLateFee + IF(objfee.AlreadyPaidRegistrationFee,0,mapOfProductAndAmount['Registration Fee']) })</label>
                                                    </div>
                                                </div>
                                            </apex:outputpanel>
            
                                            <apex:outputpanel rendered="{!CONTAINS(setOfPaymentOption,'5SemFee')}" styleClass="col-xs-12 col-sm-12 col-md-12 col-lg-12" layout="block">
                                                <div class="form-group form-group-lg m-b-0 rm-m-b">
                                                    <div class="" >
                                                        <input name="group1" type="radio" class="with-gap form-control" id="radio_9"  data-mode="1" data-value="5SemFee" data-label="{!IF(objfee.AlreadyPaidRegistrationFee,'','Registration + ')} {!IF(objDetails.isTerm,'5TermFee','5SemFee')} {!IF(objDetails.CareerServiceOpted,' + Career Services','')} {!IF(objDetails.applicableLateFee > 0,' + Late Fee' ,'')}"/>
                                                        <label for="radio_9">{!IF(objfee.AlreadyPaidRegistrationFee,'','Registration + ')} {!IF(objDetails.isTerm,'5TermFee','5SemFee')} {!IF(objDetails.CareerServiceOpted,' + Career Services','')} {!IF(objDetails.applicableLateFee > 0,' + Late Fee' ,'')} (INR {! mapOfProductAndAmount['5SemFee'] + objDetails.applicableCareerServiceFee + objCoursera.applicableCourseraFee+ objDetails.applicableLateFee + IF(objfee.AlreadyPaidRegistrationFee,0,mapOfProductAndAmount['Registration Fee']) })</label>
                                                    </div>
                                                </div>
                                            </apex:outputpanel>
            
                                            <apex:outputpanel rendered="{!CONTAINS(setOfPaymentOption,'MultipleSemFee')}" styleClass="col-xs-12 col-sm-12 col-md-12 col-lg-12" layout="block">
                                                <div class="form-group form-group-lg m-b-0 rm-m-b">
                                                    <div class="" >
                                                        <input name="group1" type="radio" class="with-gap form-control" id="radio_10"  data-mode="1" data-value="MultipleSemFee" data-label="{!IF(objfee.AlreadyPaidRegistrationFee,'','Registration + ')} Multiple Sem Fee {!IF(objDetails.CareerServiceOpted,' + Career Services','')} {!IF(objCoursera.checkCourseraOpted,' + Coursera','')} {!IF(objDetails.applicableLateFee > 0,' + Late Fee' ,'')}"/>
                                                        <label for="radio_10">{!IF(objfee.AlreadyPaidRegistrationFee,'','Registration + ')} Multiple Sem Fee {!IF(objDetails.CareerServiceOpted,' + Career Services','')} {!IF(objCoursera.checkCourseraOpted,' + Coursera','')} {!IF(objDetails.applicableLateFee > 0,' + Late Fee' ,'')} (INR {! mapOfProductAndAmount['MultipleSemFee'] +objDetails.applicableCareerServiceFee+ objCoursera.applicableCourseraFee+ objDetails.applicableLateFee + IF(objfee.AlreadyPaidRegistrationFee,0,mapOfProductAndAmount['Registration Fee']) })</label>
                                                    </div>
